model Specialties {
  id    String @id @default(ulid())
  title String
  file  String

  doctorSpecialties DoctorSpecialty[]

  @@map("specialties")
}

model DoctorSpecialty {
  specialtysId String
  specialities Specialties @relation(fields: [specialtysId], references: [id])

  doctorId String
  doctor   Doctor @relation(fields: [doctorId], references: [id])

  @@id([specialtysId, doctorId])
  @@map("doctor_specialties")
}

